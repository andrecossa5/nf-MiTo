{
    "description": "High-stringency variant detection for high-coverage data",
    "use_case": "High-quality datasets where precision is more important than sensitivity",
    
    "input_output_options": {
        "raw_data_input_type": "mitobam",
        "output_folder": "results/high_stringency"
    },
    
    "cell_filtering_options": {
        "min_nUMIs": 1000,
        "min_n_genes": 500,
        "max_perc_mt": 0.10,
        "n_mads": 2
    },
    
    "afm_preprocessing_options": {
        "filter_dbs": true,
        "cell_filter": "filter2",
        "filtering": "MiTo",
        "min_cell_number": 10,
        "min_cov": 10,
        "min_var_quality": 40,
        "min_n_positive": 10,
        "af_confident_detection": 0.05,
        "min_n_confidently_detected": 5,
        "min_mean_AD_in_positives": 2.0,
        "t_prob": 0.8,
        "min_AD": 3,
        "min_cell_prevalence": 0.1,
        "bin_method": "MiTo"
    },
    
    "phylogeny_reconstruction_options": {
        "distance_metric": "weighted_jaccard",
        "tree_algorithm": "cassiopeia",
        "cassiopeia_solver": "UPMGA",
        "n_boot_replicates": 1000,
        "boot_strategy": "feature_resampling",
        "frac_char_resampling": 0.9
    }
}